{
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "es6": true
    },
    "globals": {
    },
    "rules": {
        "brace-style": [2, "1tbs", { "allowSingleLine": true }],
        "comma-spacing": [2, {"before": false, "after": true}],
        "comma-style": [2, "last"],
        "curly": 2,
        "dot-notation": 1,
        "eol-last": 0,
        "eqeqeq": 2,
        "func-style": [2, "declaration"],
        "guard-for-in": 2,
        "key-spacing": [2, {"beforeColon": false, "afterColon": true, "align": "value"  }],
        "new-cap": 2,
        "no-caller": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-strict": 0,
        "no-multi-spaces": 0,
        "no-native-reassign": 2,
        "no-plusplus": 2,
        "no-redeclare": 2,
        "no-self-compare": 2,
        "no-trailing-spaces": 0,
        "no-unused-vars": [2, {"vars": "all", "args": "none"}],
        "no-use-before-define": [2, "nofunc"],
        "no-void": 0,
        "no-with": 2,
        "quotes": 0,
        "radix": 0,
        "semi": [2, "always"],
        "semi-spacing": [2, {"before": false, "after": true}],
        "space-after-keywords": [1, "always"],
        "space-infix-ops": 2,
        "strict": [1, "function"],
        "vars-on-top": 2,
        "wrap-iife": [2, "outside"]
    }
}